<!--pages/workSta/workSta.wxml-->
<view class="line" hidden="{{list.length == 0}}"></view>
<view class="item flex" wx:for="{{list}}" wx:key="unique">
    <view class="item-icon">
        <image class="item-icon_img" src="{{icon_arrow}}" model="scaleToFill" ></image>
    </view>
    <view class="item-cnt flex">
        <view class="item-date flex">
            <view>{{item.year}}</view>
            <view>{{item.month}}</view>
        </view>
        <view class="item-text flex">
            <text>{{item.text}}</text> 
        </view>
    </view>
</view>

