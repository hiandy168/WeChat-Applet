/**app.wxss**/
@import './components/wetoast/wetoast.wxss';
page{
  background-color: #efeff4;
}
.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
} 
.txt-hide{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.flex{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
}
.panel-bar{
  height:60rpx;
  opacity: 0;
}
